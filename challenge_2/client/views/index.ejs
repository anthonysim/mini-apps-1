<!doctype html>
<html lang="en">

<!-- header -->
<%- include('./header'); %>

  <body style="font-family: 'Open Sans', sans-serif;">

    <!-- Title on top of Web Page -->
    <h1 style="text-align: center">CSV Report Generator</h1>

    <!-- HTML Form -->
    <%- include('./form'); %>

      <!-- Download Link -->
      <%- include('./downloadLink'); %>

        <!-- table for the data! -->
        <table>
          <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>County</th>
            <th>City</th>
            <th>Role</th>
            <th>Sales</th>
          </tr>
          <tr>
            <td>
              <%= data[0].firstName %>
            </td>
            <td>
              <%= data[0].lastName %>
            </td>
            <td>
              <%= data[0].county %>
            </td>
            <td>
              <%= data[0].city %>
            </td>
            <td>
              <%= data[0].role %>
            </td>
            <td>
              <%= data[0].sales %>
            </td>
          </tr>
        </table>


        <!-- JS File -->
        <script src=" app.js"></script>

        <!-- EXPRESS SERVER -->


  </body>

</html>